@article{VansteelandtVanderWeeleTchetgen:2008,
  title = {Multiply {{Robust Inference}} for {{Statistical Interactions}}},
  author = {Vansteelandt, Stijn and VanderWeele, Tyler J. and Tchetgen, Eric J. and Robins, James M.},
  year = {2008},
