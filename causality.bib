  keywords = {bias,Bias,Causality,Computer Simulation,confounding,{Data Interpretation, Statistical},Depression,Humans,inverse probability weighting,life course,marginal structural models,{Models, Structural},{Models, Theoretical},Risk Factors,simulation,social epidemiology,Stroke,To Read},
