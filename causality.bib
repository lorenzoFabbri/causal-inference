@article{RudolphBenkeserKennedy:2022,
  title = {Estimation of the {{Average Causal Effect}} in {{Longitudinal Data With Time-Varying Exposures}}: {{The Challenge}} of {{Nonpositivity}} and the {{Impact}} of {{Model Flexibility}}},
